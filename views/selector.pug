extends layout

block content
  h1= title
  - var image_names = ["earring1.jpg","earring2.jpg","earring3.jpg","earring4.jpg","earring5.jpg"]
  - for(let i=0;i<3;i++){
     - let random= Math.floor(Math.random()*5)
     img(src=`/images/${image_names[random]}` alt=`Image No ${random}` height="300px" width="300px")
  - }